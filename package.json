  "dependencies": {
    "axios": "^0.16.2",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.17.2",
    "compression": "^1.7.0",
    "cron": "^1.2.1",
    "express": "^4.15.4",
    "goo.gl": "^0.1.4",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.0.0",
    "ioredis": "^4.0.0",
    "jsonwebtoken": "^8.0.0",
    "jwt-decode": "^2.2.0",
    "konphyg": "^1.4.0",
    "material-ui": "^0.19.0",
    "nodemailer": "^4.1.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.2.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-error-overlay": "^1.0.10",
    "react-router-dom": "^4.3.1",
    "react-tooltip": "^3.3.0",
    "twilio": "^3.6.6",
    "validator": "^8.1.0"
  },
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "^3.0.2",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "concurrently": "^3.5.0",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-react-app": "^2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.3.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "global": "^4.3.2",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "node-sass": "^4.13.1",
    "nodemon": "^1.12.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "react-dev-utils": "^3.1.0",
    "sass-loader": "^6.0.6",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "server": "nodemon src/server/server.js",
    "client": "node config/scripts/start.js",
    "build": "node config/scripts/build.js",
    "start-local": "node src/server/server.js",
    "start": "NODE_ENV=production pm2 start src/server/server.js --name ym",
    "delete": "pm2 delete ym",
    "restart": "pm2 restart ym",
    "logs": "pm2 logs ym --lines 100",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
